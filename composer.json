{
  "name": "awesomeobjects/awesomeobjects",
  "description": "A package containing a range of awesome objects!",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Stephen Mitchell",
      "email": "stephen.p.mitchell@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^11"
  },
  "autoload": {
    "psr-4": {
      "AwesomeObjects\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AwesomeObjects\\Test\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.x-dev"
    }
  }
}